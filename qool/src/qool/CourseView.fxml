<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<GridPane hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="400.0" prefWidth="600.0" fx:controller="qool.CourseController">
    <Label text="Title" GridPane.rowIndex="0" GridPane.columnIndex="0"  />
    <TextField fx:id="title" promptText="Course Title" GridPane.rowIndex="0" GridPane.columnIndex="1" />  
    <Button fx:id="submit" text = "Save" GridPane.rowIndex="1" GridPane.columnIndex="1" onAction="#save" />  
</GridPane>    
